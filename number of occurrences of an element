#include<iostream>
using namespace std;
int main()
{
    int i,s,ar[20],count=0,n;
    cout<<"Enter the no.of elements:";
    cin>>n;
    cout<<"Enter the element:";
    cin>>s;
    cout<<"Enter the elements:";
     for(i=0;i<n;i++)
     {
         cin>>ar[i];
     }
    for(i=0;i<n;i++)
    {
        if(ar[i]==s)
        {
            count++;
        }
    }
    cout<<"Count:"<<count<<endl;
    return 0;
}
